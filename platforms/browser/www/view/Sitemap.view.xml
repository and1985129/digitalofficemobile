<mvc:View
	controllerName="sap.dm.controller.Sitemap"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:h="http://www.w3.org/1999/xhtml">
	<Page
		title="Site Map"
		navButtonPress="handleNavBack"
		showNavButton="{device>/isPhone}"
		showFooter="{device>/isNoPhone}">
		<headerContent>
		</headerContent>
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
		<content>
			<Image
				densityAware="false"
				src="img/bigmap.png"></Image>
		</content>
	</Page>
</mvc:View>